{
  "name": "reactpress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "components-build": "cd packages/components && npm run compile",
    "components-watch": "cd packages/components && npm run watch",
    "init": "npm install && lerna bootstrap && npm run components-build",
    "components": "cd packages/components && npm run storybook",
    "frontend": "cd packages/frontend && npm run dev",
    "dev": "concurrently \"npm run frontend\" \"npm run components-watch\" \"npm run components\"",
    "now-build": "npm run init && cd packages/frontend && npm run build",
    "now-start": "cd packages/frontend && npm run start",
    "deploy": "now -e NODE_ENV=production"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lerna": "^3.4.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
